<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.collections.FXCollections?>
<?import java.lang.String?>
<VBox xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.coursework.Controllers.CtrlsForExecutionStages.DeleteCtrl">

    <Button text="Исполнить" onAction="#button1Clicked" />

    <ChoiceBox fx:id="choiceBox">
        <items>
